{
	"name": "Halo Notification Service",
	"description": "Makes Halo better",
	"version": "1.0",
	"manifest_version": 3,
	"host_permissions": ["*://halo.gcu.edu/"],
	"permissions": ["cookies", "identity", "storage"],
	"action": {
		"default_popup": "popup/popup.html"
	},
	"background": {
        "service_worker": "background.js",
		"type": "module"
    }
}

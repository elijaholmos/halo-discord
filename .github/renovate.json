{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		":ignoreModulesAndTests",
		":autodetectPinVersions",
		":prConcurrentLimit10",
		"group:recommended",
		"workarounds:all",
		":semanticCommitType(chore)"
	],
	"automerge": true,
	"automergeType": "pr-comment",
	"automergeComment": "@renovate merge",
	"automergeStrategy": "squash"
}
